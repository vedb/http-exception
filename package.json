{
  "name": "http-exception",
  "version": "0.0.2",
  "description": "A small, easy-to-use, and dependency-free http errors library for Node",
  "main": "dist/index.js",
  "author": "Vedran Burdzovic",
  "license": "MIT",
  "repository": "https://github.com/vedrxn/http-exception.git",
  "scripts": {
    "format": "prettier --write '**/*{.js,.md}'",
    "test": "mocha"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "http",
    "error",
    "exception"
  ],
  "devDependencies": {
    "husky": "^4.0.6",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.21",
    "mocha": "^8.4.0",
    "prettier": "^2.3.0"
  }
}
