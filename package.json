    "exception"
  ],
  "devDependencies": {
    "husky": "^4.0.6",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "mocha": "^7.0.0",
    "prettier": "^1.19.1"
  }
}
